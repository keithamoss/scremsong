{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "no-console": false,
        "ban-types": false,
        "object-literal-sort-keys": false,
        "no-empty-interface": false
    },
    "linterOptions": {
        "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
    }
}
